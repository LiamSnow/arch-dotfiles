[calendars]

[[general]]
path = /home/liams/.calendars/general/general/
type = calendar

[[school]]
path = /home/liams/.calendars/school/school/
type = calendar

[locale]
timeformat = %I:%M %p
dateformat = %m/%d/%Y
longdateformat = %m/%d/%Y
datetimeformat = %m/%d/%Y %I:%M %p
longdatetimeformat = %m/%d/%Y %I:%M %p

[default]
default_calendar = general
