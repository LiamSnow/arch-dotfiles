[calendars]

[[calendars]]
path = /home/liams/.calendars/*
type = discover

[locale]
firstweekday = 6
timeformat = %I:%M %p
dateformat = %m/%d/%Y
longdateformat = %m/%d/%Y
datetimeformat = %m/%d/%Y %I:%M %p
longdatetimeformat = %m/%d/%Y %I:%M %p

[default]
default_calendar = General
highlight_event_days = True

[highlight_days]
method = fg
multiple = "#b2ec5d"

[view]
frame = top
theme = dark
